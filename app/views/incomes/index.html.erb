<h1>Incomes</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Income date</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.id %></td>
        <td><%= link_to income.income_date, income_path(income) %></td>
        <td><%= income.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_income_path(income), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', income_path(income), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_income_path, :class => 'btn btn-primary' %>
